{
    "name": "linderp/sulu-swiss-qr-bill-bundle",
    "description": "Swiss QR Bill generator for Sulu CMS",
    "type": "symfony-bundle",
    "version": "0.1",
    "require": {
        "php": "^8.2",
        "sulu/sulu": "^2.6",
        "symfony/config": "^6.2 | ^7.0",
        "symfony/dependency-injection": "^6.2 | ^7.0",
        "symfony/framework-bundle": "^6.2 | ^7.0",
        "symfony/http-foundation": "^6.2 | ^7.0",
        "symfony/http-kernel": "^6.2 | ^7.0",
        "symfony/intl": "^6.2 | ^7.0",
        "symfony/translation": "^6.2 | ^7.0",
        "symfony/security-core": "^6.3 | ^7.0",
        "jackalope/jackalope-doctrine-dbal": "^2.0",
        "sprain/swiss-qr-bill": "^5.2.0",
        "tecnickcom/tc-lib-pdf": "^8.4.0",
        "tecnickcom/tcpdf": "^6.10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Linderp\\SuluSwissQRBillBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Pascal Linder",
            "email": "pascal.linder@web-venture.ch"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": true
        }
    }
}
